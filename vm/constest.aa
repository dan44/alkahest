; add_list
.add_list
    ld r5,*0
    REGISNIL 1 isnil2
    ld r2,r1
.loop1
    REGISNIL 2 loop1exit
    caar r7,r2
    cmpi r7,r6
    jnz continue
    ld r1,r2
    jr end1
.continue
    ld r5,r2
    cdr r2,r2
    jr loop1
.loop1exit
.isnil2
    ld r1,^cons
    ld r2,^cons
    REGISNIL 5 isnil1
    cdwr r5,r1
.isnil1
    cawr r1,r2
    car r5,r1
    cawr r5,r6
    cdwar r1,*0
    cdwr r1,*0
    ld r2,*0
    ld r5,*0
.end1
    halt
